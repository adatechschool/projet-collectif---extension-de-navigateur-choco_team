{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "This is a weather extension.",
    "permissions": ["activeTab"],
        "action":{
        "default_popup": "index.html",
        "default_icon": {
            "16":"image/icon-16.png",
            "32":"image/icon-32.png",
            "48":"image/icon-48.png",
            "128":"image/icon-128.png"

        }
    },

    "icons": {
        "16":"image/icon-16.png",
        "32":"image/icon-32.png",
        "48":"image/icon-48.png",
        "128":"image/icon-128.png"
}
}